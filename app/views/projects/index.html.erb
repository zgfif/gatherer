<h1>All Projects</h1>
<table>
  <thead>
    <tr>
      <td>Project Name</td>
      <td>Project Size</td>
    </tr>
  </thead>
  <tbody>
    <% ProjectPresenter.from_project_list(@projects).each do |project| %>
      <tr class='project-row' id=<%= dom_id(project) %> >
        <td class='name'><%= name_with_status(project) %></td>
        <td class='total-size'><%= project.size %></td>
      </tr>
    <% end %>
  </tbody>
</table>
